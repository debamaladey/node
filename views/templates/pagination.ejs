<% if (pageCount > 1) { %>  
    <ul class="pagination">  
      <% if (currentPage > 1) { %>
          <li><a href="/home-list/?page=1">First</a></li>
      <% } %>
      <% var i = 1;
      if (currentPage > 5) {
      i = +currentPage - 4;
      } %>
      <% if (i !== 1) { %>
          <li class="disabled"><a href="#">...</a></li>
      <% } %>
      <% for (i; i<=pageCount; i++) { %>
        <% if (currentPage == i) { %>
            <li class="active"><span><%= i %> <span class="sr-only">(current)</span></span></li>
        <% } else { %>
            <li><a href="/home-list/?page=<%= i %>"><%= i %></a></li>
        <% } %>
        <% if (i == (+currentPage + 4)) { %>
            <li class="disabled"><a href="#">...</a></li>
        <% break; } %>
      <% } %>
      <% if (currentPage != pageCount) { %>
          <li><a href="/home-list/?page=<%= pageCount %>">Last</a></li>
      <% } %>
    </ul>  
<% } %> 